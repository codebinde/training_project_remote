{<<"app">>,<<"xmlrpc">>}.
{<<"build_tools">>,[<<"mix">>]}.
{<<"description">>,
 <<"XML-RPC encoder/decder for Elixir. Supports all valid datatypes. Input (ie untrusted) is parsed with erlsom against an xml-schema for security.">>}.
{<<"elixir">>,<<"~> 1.4">>}.
{<<"files">>,
 [<<"lib">>,<<"lib/xml_rpc">>,<<"lib/xml_rpc/encoder.ex">>,
  <<"lib/xml_rpc/decoder.ex">>,<<"lib/xml_rpc/base64.ex">>,
  <<"lib/xml_rpc/date_time.ex">>,<<"lib/xml_rpc/xmlrpc.xsd">>,
  <<"lib/xml_rpc.ex">>,<<"mix.exs">>,<<"README.md">>,<<"LICENSE">>]}.
{<<"licenses">>,[<<"Apache-2.0">>]}.
{<<"links">>,
 [{<<"GitHub">>,<<"https://github.com/ewildgoose/elixir-xml_rpc">>}]}.
{<<"name">>,<<"xmlrpc">>}.
{<<"requirements">>,
 [[{<<"app">>,<<"erlsom">>},
   {<<"name">>,<<"erlsom">>},
   {<<"optional">>,false},
   {<<"repository">>,<<"hexpm">>},
   {<<"requirement">>,<<"~> 1.4">>}],
  [{<<"app">>,<<"decimal">>},
   {<<"name">>,<<"decimal">>},
   {<<"optional">>,false},
   {<<"repository">>,<<"hexpm">>},
   {<<"requirement">>,<<"~> 1.0">>}]]}.
{<<"version">>,<<"1.4.2">>}.
